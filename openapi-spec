  /bcf/2.1/projects/{projectId}/topics:
    get:
      tags:
        - Topics (BCF 2.1)
      summary: GetTopics
      description:
        "Retrieve a collection of topics related to the specified Trimble Connect Project (default sort order is \"modified_date\")."
      operationId: "ListAllTopics"
      parameters:
        - name: projectId
          in: path
          description: The id of the Trimble Connect Project
          required: true
          schema:
            type: string
        - name: top
          in: query
          description: The maximum number of items to be returned on a single page
          schema:
            type: integer
            format: int32
            default: 500
            maximum: 500
        - name: skiptoken
          in: query
          description: The token used for pagination. Typically returned with the response in the previous page and should be used as-is without interpretation.
          schema:
            type: string
      responses:
        "200":
          description: "An array of Topics for this Project."
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/TopicDto'
        "204":
          description: "No Topics have been defined yet for this Project."
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
    post:
      tags:
        - Topics (BCF 2.1)
      summary: CreateTopic
      description:
        "Creates a new Topic for the specified Trimble Connect Project.\r\n
        \r\nSample request:\r\n
        \r\n    POST Projects/{projectId}/Topics
        \r\n    {
        \r\n        \"title\": \"Clash between pipes\",
        \r\n        \"topic_type\": \"Inspection\",
        \r\n        \"description\": \"More details needed\",
        \r\n        \"assigned_to\": \"Assignee_Name@trimble.com\",
        \r\n        \"assigned_to_uuid\": \"c5a73cc9-b155-4556-8963-bc7dc13c6fd2\",
        \r\n        \"topic_status\": \"Not Started\",
        \r\n        \"reference_links\": [ \"https://www.google.com\", \"https://www.trimble.com\" ],
        \r\n        \"labels\": [ \"Road\", \"Electrical\" ]
        \r\n    }"
      operationId: "CreateNewTopic"
      parameters:
        - name: projectId
          in: path
          description: The id of the Trimble Connect Project
          required: true
          schema:
            type: string
        - name: validation
          in: query
          description:
            "A flag to customize the validation behavior.\r\n
            \r\nUse 'add' to add new values from the new topic to the project extensions.\r\n
            \r\nUse 'replace' to replace the topic values not present in the project extensions with current default values from the project extensions. If there is no default value, no value will be set.\r\n
            \r\nUse 'strict' to validate the topic values against the current project extensions values (this is the default behavior). If the topic values are not found in the project extensions, it will result in a validation error."
          schema:
            type: string
            default: strict
      requestBody:
        description: The Topic to be created
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/CreateTopicDto'
          text/json:
            schema:
              $ref: '#/components/schemas/CreateTopicDto'
          application/*+json:
            schema:
              $ref: '#/components/schemas/CreateTopicDto'
      responses:
        "201":
          description: "The newly created Topic."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/TopicDto'
        "400":
          description: "Request is invalid."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ErrorResponse'
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "415":
          description: "Unsupported Media Type."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
