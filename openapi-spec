# Foundation
  /foundation/versions:
    get:
      summary: An unauthenticated API discovery endpoint
      tags:
        - Foundation
      description: >
           Returns a list of all supported APIs and their versions on this server.
      responses:
        '200':
          description: OK
          content:
            "application/json":
              schema:
                "$ref": "#/components/schemas/VersionsResponse"
        '403':
          description: Forbidden
          content:
            "*/*":
              schema:
                "$ref": "#/components/schemas/errorResponse"
  /foundation/1.0/auth:
    get:
      summary: An unauthenticated OAUTH discovery endpoint.
      tags:
        - Foundation
      responses:
        '200':
          description: OK
          content:
            "*/*":
              schema:
                "$ref": "#/components/schemas/AuthenticationInfoResponse"
        '403':
          description: Forbidden
          content:
            "*/*":
              schema:
                "$ref": "#/components/schemas/errorResponse"
  /foundation/1.0/current-user:
    get:
      summary: Get the current user information
      tags:
        - Foundation
      responses:
        '200':
          description: OK
          content:
            "*/*":
              schema:
                "$ref": "#/components/schemas/UserResponse"
        '403':
          description: Forbidden
          content:
            "*/*":
              schema:
                "$ref": "#/components/schemas/errorResponse"

# BCF 3.0
  /bcf/3.0/projects:
    get:
      description: Retrieve a collection of projects where the currently logged on user has access to.
      summary: Get Projects
      tags:
        - Projects (BCF 3.0)
      responses:
        '200':
          description: OK
          headers:
            odata.nextLink:
              schema:
                type: string
              description: The odata nextLink.
              example: "/bcf/3.0/projects?skiptoken=q1byyUxySU3z"
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/projects'
              example:
                [{
                    "project_id": "F445F4F2-4D02-4B2A-B612-5E456BEF9137",
                    "name": "Example project 1",
                    "authorization": {
                        "project_actions": [
                            "createTopic",
                            "createDocument"
                        ]
                    }
                }, {
                    "project_id": "A233FBB2-3A3B-EFF4-C123-DE22ABC8414",
                    "name": "Example project 2",
                    "authorization": {
                        "project_actions": []
                    }
                }]
        '401':
          description: Authorization information is missing, invalid or expired.
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
        '403':
          description: User has no permissions to execute this operation.
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
  /bcf/3.0/projects/{project_id}:
    parameters:
    - $ref:  "#/components/parameters/project_id"
    get:
      summary: Get Project
      description: Retrieve a specific project.
      tags:
        - Projects (BCF 3.0)
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/project_GET'
              example:
                {
                    "project_id": "B724AAC3-5B2A-234A-D143-AE33CC18414",
                    "name": "Example project 3",
                    "authorization": {
                        "project_actions": [
                            "update"
                        ]
                    }
                }
        '400':
          description: |
            Bad Request.
            Possible reasons include:
            - Path parameters passed in a format that is not supported
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
        '401':
          description: Authorization information is missing, invalid or expired.
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
        '403':
          description: User has no permissions to execute this operation.
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
        '404':
          description: |
            Not Found - The requested resource could not be found.
            Possible reasons include:
            - `project_id` in path parameter not found
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/errorResponse"
  /bcf/3.0/projects/{project_id}/extensions:
    parameters:
    - $ref:  "#/components/parameters/project_id"
    get:
      summary: Get Project Extensions
      description:  |
        Retrieve a specific projects extensions. Project extensions are used to define possible values that can be used in topics and comments, for example topic labels and priorities.
        They may change during the course of a project. The most recent extensions state which values are valid at a given moment for newly created topics and comments.
      tags:
        - Projects (BCF 3.0)
      responses:
        '200':
          description: |
            The response format varies based on the `Accept` header value in the request.

            | Accept Header Value                              | Response Format                      |
            |--------------------------------------------------|--------------------------------------|
            | Not provided or `application/json`               | Pure BCF 3.0 representation          |
            | `application/vnd.trimble.connect.app+json`       | Connect representation               |
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/extensions_GET'
              example:
                {
                    "topic_type": [
                      "Information",
                      "Error"
                    ],
                    "topic_status": [
                      "Open",
                      "Closed",
                      "ReOpened"
                    ],
                    "topic_label": [
                      "Architecture",
                      "Structural",
                      "MEP"
                    ],
                    "snippet_type": [
                      ".ifc",
                      ".csv"
                    ],
                    "priority": [
                      "Low",
                      "Medium",
                      "High"
                    ],
                    "users": [
                      "Architect@example.com",
                      "BIM-Manager@example.com",
                      "bob_heater@example.com"
                    ],
                    "stage": [
                      "Preliminary Planning End",
                      "Construction Start",
                      "Construction End"
                    ],
                    "project_actions": [
                      "update",
                      "createTopic",
                      "createDocument"
                    ],
                    "topic_actions": [
                      "update",
                      "updateBimSnippet",
                      "updateRelatedTopics",
                      "updateDocumentReferences",
                      "updateFiles",
                      "createComment",
                      "createViewpoint"
                    ],
                    "comment_actions": [
                      "update"
                    ]
                }
            application/vnd.trimble.connect.app+json:
              schema:
                $ref: '#/components/schemas/extensions_GET_Connect'
              example:
                {
                  "topic_type": [
                    "Information",
                    "Error"
                  ],
                  "topic_status": [
                    "Open",
                    "Closed",
                    "ReOpened"
                  ],
                  "topic_label": [
                    "Architecture",
                    "Structural",
                    "MEP"
                  ],
                  "snippet_type": [
                    ".ifc",
                    ".csv"
                  ],
                  "priority": [
                    "Low",
                    "Medium",
                    "High"
                  ],
                  "users": [
                    "Architect@example.com",
                    "BIM-Manager@example.com",
                    "bob_heater@example.com"
                  ],
                  "stage": [
                    "Preliminary Planning End",
                    "Construction Start",
                    "Construction End"
                  ],
                  "topic_server_assigned_id_prefix" : "ABCD",
                  "project_actions": [
                    "update",
                    "createTopic",
                    "createDocument"
                  ],
                  "topic_actions": [
                    "update",
                    "updateBimSnippet",
                    "updateRelatedTopics",
                    "updateDocumentReferences",
                    "updateFiles",
                    "createComment",
                    "createViewpoint"
                  ],
                  "comment_actions": [
                    "update"
                  ],
                  "topic_type_full": [
                    {
                      "name": "Comment",
                      "color": "#005F9E",
                      "icon": "data:image/svg+xml;base64,XXXXXXXX",
                      "visible": true,
                      "isdefault": false
                    },
