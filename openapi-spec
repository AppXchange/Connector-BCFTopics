components:
  schemas:
    UserResponse:
      type: object
      required: ["id"]
      properties:
        id:
          type: string
          description: A unique identifier for the user on the server
          example: Architect@example.com
        name:
          type: string
          description: The user's full name
          example: "John Doe"
    AuthenticationInfoResponse:
      type: object
      required: [ 'supported_oauth2_flows' ]
      properties:
        oauth2_auth_url:
          type: string
          description: 'URL to authorization page (used for Authorization Code Grant and Implicit Grant OAuth2 flows)'
          example: "https://example.com/foundation/oauth2/auth"
        oauth2_token_url:
          type: string
          description: URL for token requests
          example: https://example.com/foundation/oauth2/token
          format: uri
        http_basic_supported:
          type: boolean
          description: Indicates if Http Basic Authentication is supported
          example: false
        supported_oauth2_flows:
          type: array
          description: Indicates supported OAuth2 flows
          items:
            type: string
            enum: ["authorization_code_grant", "implicit_grant", "resource_owner_password_credentials_grant"]
    Version:
      type: object
      required: ['api_id', 'version_id']
      properties:
        version_id:
          type: string
          description: The version number
          example: '3.0'
        api_id:
          type: string
          description: The API name
          enum: [bcf, foundation]
          example: bcf
        detailed_version:
          type: string
          description: The URL of the API specification on Github
          example: 'https://github.com/buildingSMART/BCF-API/tree/release'
        api_base_url:
          type: string
          description: An optional, fully qualified URL, to allow servers to relocate the API
          example: 'https://server.base.url/somepath/bcf/3.0'
          format: uri
    VersionsResponse:
      type: object
      properties:
        versions:
          type: array
          items:
            "$ref": "#/components/schemas/Version"
    # custom error schema
    errorResponse:
      type: object
      description: Schema for error response, BCF REST API.
      required:
        - message
      properties:
        code:
          type: string
          description: Error code
        message:
          type: string
          description: Error message
