                $ref: "#/components/schemas/ErrorResponse"
  /bcf/2.1/projects/{projectId}/topics/{topicId}/viewpoints/{viewpointId}:
    get:
      tags:
        - Viewpoints (BCF 2.1)
      summary: GetViewpoint
      description: "Retrieve a specific viewpoint."
      operationId: "ListViewpointByViewpointId"
      parameters:
        - name: projectId
          in: path
          required: true
          schema:
            type: string
        - name: topicId
          in: path
          required: true
          schema:
            type: string
        - name: viewpointId
          in: path
          required: true
          schema:
            type: string
      responses:
        "200":
          description: "The requested viewpoint including a resized small snapshot and a url to the full snapshot."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Viewpoint'
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId/topicId/viewpointId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
    delete:
      tags:
        - Viewpoints (BCF 2.1)
      description: "API to delete the viewpoint"
      operationId: "DeleteViewpointByviewpointId"
      summary: DeleteViewpoint
      parameters:
        - name: projectId
          in: path
          description: The id of the Trimble Connect project
          required: true
          schema:
            type: string
        - name: topicId
          in: path
          description: The id of the Topic.
          required: true
          schema:
            type: string
        - name: viewpointId
          in: path
          description: The id of the viewpoint to be deleted.
          required: true
          schema:
            type: string
      responses:
        "204":
          description: "The viewpoint was deleted successfully."
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId/topicId/viewpointId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
