    PageLinks:
      type: object
      required:
        - self
        - first
      properties:
        self:
          type: object
          description: "A URL that will retrieve the response that was just received."
          properties:
            href:
              type: string
              description: "An absolute or relative url."
        first:
          type: object
          description: "A URL that will retrieve the first page of results."
          properties:
            href:
              type: string
              description: "An absolute or relative url."
        next:
          type: object
          description: "A URL that will retrieve the next page of results.
            Responses that include only a partial set of the items identified
            by the request will contain a link that allows retrieving the next partial
            set of items.
            \r\n\r\n
            This link is called a next link. Clients should treat
            the URL of the next link as opaque, and should not append system query
            options to the URL of a next link.
            \r\n\r\n
            The url returned by server might be an absolute url or path with query parameters.
            \r\n\r\n
            If it is a path,
            client is responsible for adding a hostname to form a full url.
            \r\n\r\n
            Otherwise
            no transformations must be applied by the client to the url and it should
            be sent as is to the server to fetch the next page of events."
          properties:
            href:
              type: string
              description: "An absolute or relative url."
    SyncObjectList:
      description: "The list of objects with changeToken"
      type: object
      required:
        - links
      properties:
        changeToken:
          type: string
          description:
            "The new cursor value for subsequent fetches.
            When the same requested cursor value is returned in the response, it indicates that no objects of the specified type have been created in the project yet.
            In this scenario, retrying the same API later is necessary to check for any newly created objects."
        items:
          description:
            "The objects changed since the last changeToken. This may be empty if there are no changes since the
            last changeToken."
          type: object
          oneOf:
            - $ref: '#/components/schemas/TopicList'
            - $ref: '#/components/schemas/CommentList'
            - $ref: '#/components/schemas/ViewpointList'
            - $ref: '#/components/schemas/DocumentReferenceList'
          example:
            # preview didn't work as expected: - $ref:'#/components/examples/TopicDtoExample'
            comments:
              - guid: B345F4F2-3A04-B43B-A713-5E456BEF8228
                date: 2023-08-01T17:34:22.409Z
                author: developer@trimble.com
                author_uuid: 11111111-2222-3333-4444-555555555555
                comment: example comment
                topic_guid: C1C1C1C1-3A04-B43B-A713-5E456BEF8228
              - guid: 22222222-3A04-B43B-A713-5E456BEF8228
                date: 2023-08-01T17:35:33.409Z
                author: developer@trimble.com
                author_uuid: 11111111-2222-3333-4444-555555555555
                comment: second example comment
                topic_guid: C1C1C1C1-3A04-B43B-A713-5E456BEF8228
        deletedItems:
          description:
            "The objects deleted since the last changeToken. This may be empty if there are no deletions since the
            last changeToken."
          type: object
          oneOf:
            - $ref: '#/components/schemas/DeletedTopicList'
