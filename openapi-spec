          schema:
            type: integer
            format: int32
            default: 500
            maximum: 500
        - name: skiptoken
          in: query
          description: The token used for pagination. Typically returned with the response in the previous page and should be used as-is without interpretation.
          schema:
            type: string
      responses:
        "200":
          description: "An array of document references for this topic."
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/DocumentReferenceDto'
        "204":
          description: "No document references have been defined yet for this Project."
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId/topicId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
    post:
      tags:
        - Document References (BCF 2.1)
      summary: CreateDocumentReference
      description:
        "Creates a document reference. This can be a document inside the project (identified by the document_guid) or an external reference (identified by the url).\r\n
        \r\nYou can also add a Trimble Connect clashset by prefixing the clashset id with 'CLASH-'.\r\n
        \r\nYou can also add a Trimble Connect model object by prefixing the model object id with 'OBJECT-'.\r\n
        \r\nNote: 'url' and 'document_guid' are mutually exclusive\r\n
        \r\nSample request (for a document belonging to the project):\r\n
        \r\n    POST projects/{projectId}/topics/{topicId}/document_references
        \r\n    {
        \r\n        \"document_guid\": \"LC0Ilxg7jtU\",
        \r\n        \"description\": \"Sample file.txt\"
        \r\n    }
        \r\nSample request (for a clashset belonging to the project):\r\n
        \r\n    POST projects/{projectId}/topics/{topicId}/document_references
        \r\n    {
        \r\n        \"document_guid\": \"CLASH-aA5cGgOKj24\",
        \r\n        \"description\": \"Clashset Report 1\"
        \r\n    }
        \r\nSample request (for a model object belonging to the project):\r\n
        \r\n    POST projects/{projectId}/topics/{topicId}/document_references
        \r\n    {
        \r\n        \"document_guid\": \"OBJECT-aA5cGgOKj24\",
        \r\n        \"description\": \"I-Beam 01\"
        \r\n    }
        \r\nSample request (for an external url):\r\n
        \r\n    POST projects/{projectId}/topics/{topicId}/document_references
        \r\n    {
        \r\n        \"url\": \"http://example.com/files/LegalRequirements.pdf\",
        \r\n        \"description\": \"LegalRequirements.pdf\"
        \r\n    }"
      operationId: "CreateDocumentReferenceForTopic"
      parameters:
        - name: projectId
          in: path
          description: The id of the Trimble Connect project
          required: true
          schema:
            type: string
        - name: topicId
          in: path
          description: The id of the topic
          required: true
          schema:
            type: string
      requestBody:
        description: The document reference to be created.
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/CreateDocumentReferenceDto'
          text/json:
            schema:
              $ref: '#/components/schemas/CreateDocumentReferenceDto'
          application/*+json:
            schema:
              $ref: '#/components/schemas/CreateDocumentReferenceDto'
      responses:
        "201":
          description: "The created document reference."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/DocumentReferenceDto'
        "400":
          description: "Request is invalid."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ErrorResponse'
        "401":
          description: "Authorization information is missing or invalid."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "403":
          description: "User has no permissions to execute this operation."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "404":
          description: "The specified projectId/topicId was not found."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
        "415":
          description: "Unsupported Media Type."
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/ErrorResponse"
