  /bcf/3.0/projects/{project_id}/topics:
    parameters:
    - $ref:  "#/components/parameters/project_id"
    get:
      summary: Get Topics
      description: Retrieve a collection of topics related to a project (default sort order is creation_date).
      tags:
        - Topics (BCF 3.0)
      responses:
        '200':
          description: |
            The response format varies based on the `Accept` header value in the request.

            | Accept Header Value                              | Response Format                      |
            |--------------------------------------------------|--------------------------------------|
            | Not provided or `application/json`               | Pure BCF 3.0 representation          |
            | `application/vnd.trimble.connect.import+json`    | Import representation                |
            | `application/vnd.trimble.connect.app+json`       | Connect representation               |
          headers:
            odata.nextLink:
              schema:
                type: string
              description: The odata nextLink.
              example: "/bcf/3.0/projects/1/topics?skiptoken=q1byyUxySU3z"
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/topic_GET'
              example:
                [{
                    "guid": "A245F4F2-2C01-B43B-B612-5E456BEF8116",
                    "server_assigned_id": "ISSUE-00001",
                    "creation_author": "Architect@example.com",
                    "title": "Example topic 1",
                    "labels": [
                        "Architecture",
                        "Structural"
                    ],
                    "creation_date": "2013-10-21T17:34:22.409Z",
                    "authorization": {
                      "topic_actions": [
                          "createComment",
                          "update",
                          "createComment",
                          "createViewpoint",
                          "delete"
                      ]
                    }
                  },
                  {
                    "guid": "A211FCC2-3A3B-EAA4-C321-DE22ABC8414",
                    "server_assigned_id": "ISSUE-00078",
                    "creation_author": "Second.Architect@example.com",
                    "title": "Example topic 2",
                    "labels": [
                        "Architecture",
                        "Heating",
                        "Electrical"
                    ],
                    "creation_date": "2014-11-19T14:24:11.316Z"
                }]
            application/vnd.trimble.connect.app+json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/topic_GET_Connect'
              example:
                [{
                    "guid": "A245F4F2-2C01-B43B-B612-5E456BEF8116",
                    "server_assigned_id": "ISSUE-00001",
                    "creation_author": "Architect@example.com",
                    "title": "Example topic 1",
                    "labels": [
                        "Architecture",
                        "Structural"
                    ],
                    "creation_date": "2013-10-21T17:34:22.409Z",
                    "marker" : {
                      "model_point" : {
                        "x" : 123.23,
                        "y" : 223.23,
                        "z" : 323.23
                      }
                    },
                    "assignees": [
                      {
                        "id" : "a16187vt-121d-4d38-9077-2fd49f94e30f",
                        "type" : "USER"
                      },
                      {
                        "id" : "hy3LoYSo7Rg",
                        "type" : "GROUP"
                      }
                    ],
                    "authorization": {
                      "topic_actions": [
                          "createComment",
                          "update",
                          "createComment",
                          "createViewpoint",
                          "delete"
                      ]
                    }
                  },
                  {
