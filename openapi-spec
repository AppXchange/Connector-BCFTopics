  /bcf/3.0/projects/{project_id}/topics/{topic_id}/comments:
    parameters:
    - $ref:  "#/components/parameters/project_id"
    - $ref:  "#/components/parameters/topic_id"
    - $ref:  "#/components/parameters/includeAuthorization"
    get:
      summary: Get Comments
      description: Retrieve a collection of all comments related to a topic (default ordering is date).
      tags:
        - Comments (BCF 3.0)
      responses:
        '200':
          description: |
            The response format varies based on the `Accept` header value in the request.

            | Accept Header Value                              | Response Format                      |
            |--------------------------------------------------|--------------------------------------|
            | Not provided or `application/json`               | Pure BCF 3.0 representation          |
            | `application/vnd.trimble.connect.import+json`    | Import representation                |
            | `application/vnd.trimble.connect.app+json`       | Connect representation               |
          headers:
            odata.nextLink:
              schema:
                type: string
              description: The odata nextLink.
              example: "/bcf/3.0/projects/1/topics/2/comments?skiptoken=q1byyUxySU3z"
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/comment_GET'
              example:
                [{
                    "guid": "C4215F4D-AC45-A43A-D615-AA456BEF832B",
                    "date": "2016-08-01T12:34:22.409Z",
                    "author": "max.muster@example.com",
                    "comment": "Clash found",
                    "topic_guid": "B345F4F2-3A04-B43B-A713-5E456BEF8228",
                    "viewpoint_guid": "b24a82e9-f67b-43b8-bda0-4946abf39624",
                    "modified_date": "2016-08-01T12:34:22.409Z",
                    "modified_author": "cathy1@example.com",
                    "authorization": {
                        "comment_actions": [
                            "update"
                        ]
                    }
                }, {
                    "guid": "A333FCA8-1A31-CAAC-A321-BB33ABC8414",
