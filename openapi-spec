    post:
      tags:
        - Viewpoints (BCF 2.1)
      summary: CreateViewpoint
      description:
        "Creates a new Viewpoint for the specified Trimble Connect Project and Topic.\r\n
        \r\nThis will also add viewpoint info to the Topic including a snapshot thumbnail for the first viewpoint.\r\n
        \r\nSample request:\r\n
        \r\n    POST projects/{projectId}/topics/{topicId}/viewpoints
        \r\n    {
        \r\n      \"index\": 2,
        \r\n      \"components\": {
        \r\n          \"selection\": [{
        \r\n              \"ifc_guid\": \"3BY5alwwL00QXHtY2qmHYZ\",
        \r\n              \"authoring_tool_id\": \"postman2\"
        \r\n          }],
        \r\n          \"coloring\": [{
        \r\n              \"color\": \"dddddd\",
        \r\n              \"components\": [{
        \r\n                  \"ifc_guid\": \"3BY5alwwL00QXHtY2qmHYZ\"
        \r\n              }]
        \r\n          }],
        \r\n          \"visibility\": {
        \r\n              \"default_visibility\": true,
        \r\n              \"exceptions\": [{
        \r\n                  \"ifc_guid\": \"0pQa7h_GLBZfPdI0XNo8Ru\",
        \r\n                  \"authoring_tool_id\": \"postman1\"
        \r\n              }],
        \r\n              \"view_setup_hints\": {
        \r\n                  \"spaces_visible\": true,
        \r\n                  \"space_boundaries_visible\": true,
        \r\n                  \"openings_visible\": true
        \r\n              }
        \r\n          }
        \r\n      },
        \r\n      \"perspective_camera\": {
        \r\n          \"camera_view_point\": {
        \r\n              \"x\": -1.3757544290857453,
        \r\n              \"y\": -14.084548687547457,
        \r\n              \"z\": 15.77535220227417
        \r\n          },
        \r\n          \"camera_direction\": {
        \r\n              \"x\": 0.48836276817323054,
        \r\n              \"y\": 0.6442499485024906,
        \r\n              \"z\": -0.5885947761547989
        \r\n          },
        \r\n          \"camera_up_vector\": {
        \r\n              \"x\": 0.3555637551166773,
        \r\n              \"y\": 0.4690610051624899,
        \r\n              \"z\": 0.808428221602439
        \r\n          },
        \r\n          \"field_of_view\": 60.0
        \r\n      },
        \r\n      \"lines\": [{
        \r\n          \"start_point\": {
        \r\n              \"x\": -70.56327164173126,
        \r\n              \"y\": -1.2323339469730854,
        \r\n              \"z\": 8.971195667982101
        \r\n           },
        \r\n          \"end_point\": {
        \r\n              \"x\": 1.657606933938703,
        \r\n              \"y\": 4.600000020209701,
        \r\n              \"z\": -42.78508872840449
        \r\n           }
        \r\n      }],
        \r\n      \"clipping_planes\": [{
        \r\n          \"location\": {
        \r\n              \"x\": 0.07161226000374072,
        \r\n              \"y\": -0.03746797100157486,
        \r\n              \"z\": 0.005424462783888953
        \r\n           },
        \r\n          \"direction\": {
        \r\n              \"x\": 0.5187780438727118,
        \r\n              \"y\": 0.8538336362389529,
        \r\n              \"z\": 0.042865636850163407
        \r\n           }
        \r\n      }],
        \r\n      \"snapshot\": {
        \r\n          \"snapshot_type\": \"png\",
        \r\n          \"snapshot_data\": \"\"
        \r\n      }
        \r\n  }"
      operationId: "CreateNewViewpoint"
      parameters:
        - name: projectId
          in: path
          description: The id of the Trimble Connect Project
          required: true
          schema:
            type: string
        - name: topicId
          in: path
          description: The id of the Topic
          required: true
          schema:
            type: string
      requestBody:
        description: The Viewpoint to be created
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/CreateViewpointDto'
          text/json:
            schema:
              $ref: '#/components/schemas/CreateViewpointDto'
          application/*+json:
            schema:
              $ref: '#/components/schemas/CreateViewpointDto'
      responses:
        "201":
          description: "The newly created Viewpoint including a resized small snapshot and a url to the full snapshot."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Viewpoint'
        "400":
          description: "Request is invalid."
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ErrorResponse'
