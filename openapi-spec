        \r\n        \"guid\": \"3fa85f64-5717-4562-b3fc-2c963f66afa6\",
        \r\n        \"date\": \"2021-05-04T08:23:21.860Z\",
        \r\n        \"author\": \"Author_Name@trimble.com\",
        \r\n        \"author_uuid\": \"a16194da-363e-4d38-9077-2fd49f94e18f\",
        \r\n        \"viewpoint_guid\": \"3fa85f64-5717-4562-b3fc-2c963f66afa6\",
